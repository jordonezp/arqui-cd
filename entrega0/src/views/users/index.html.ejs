<a href="/">Back</a>
<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last Name</th>
      <th></th>
    </tr>
    <tbody>
      <% users.forEach((user) => { %>
        <tr>
          <td><%= user.firstName %></td>
          <td><%= user.lastName %></td>
        </tr>
      <% }) %>
    </tbody>
  </thead>
</table>